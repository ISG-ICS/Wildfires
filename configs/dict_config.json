{
  "version": 1,
  "disable_existing_loggers": 0,
  "handlers": {
    "actions": {
      "level": "DEBUG",
      "class": "logging.FileHandler",
      "filename": "./taskManager_info.log",
      "formatter": "beautiful",
      "mode": "a",
      "delay": "True"
    },
    "console": {
      "level": "WARNING",
      "class": "logging.StreamHandler",
      "formatter": "simple"
    },
    "errors": {
      "class": "logging.FileHandler",
      "filename": "./taskManager_error.log",
      "level": "ERROR",
      "formatter": "beautiful",
      "mode": "a",
      "delay": "True"
    }
  },
  "formatters": {
    "beautiful": {
      "format": "[%(asctime)s] [%(levelname)5s] [%(threadName)10s] [%(module)22s] [%(funcName)15s]: %(message)s"
    },
    "simple": {
      "format": "%(levelname)s %(message)s"
    },
    "detailed": {
      "format": "%(asctime)s %(name)-15s %(levelname)-8s %(processName)-10s %(message)s"
    }
  },
  "loggers": {
    "TaskManager": {
      "level": "DEBUG",
      "propagate": 1
    },
    "YourFault": {
      "handlers": [
        "console"
      ],
      "level": "ERROR"
    }
  }
}
